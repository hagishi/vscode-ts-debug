{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "removeComments": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "inlineSourceMap": false,
    "sourceMap": false,
    "outDir": "../out",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noUnusedParameters": false
  }
}
